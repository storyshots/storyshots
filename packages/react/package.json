{
  "name": "@storyshots/react",
  "version": "0.0.19",
  "description": "React storyshots facade",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./legacy": {
      "types": "./lib/legacy.d.ts",
      "require": "./lib/legacy.js",
      "default": "./lib/legacy.js"
    }
  },
  "repository": "https://github.com/storyshots/storyshots.git",
  "license": "MIT",
  "files": [
    "lib",
    "legacy"
  ],
  "scripts": {
    "build": "rimraf ./lib && tsc --noEmit && ts-node build.ts",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@storyshots/core": "0.0.19",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17"
  }
}
